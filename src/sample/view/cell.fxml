<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootAnchorPane" prefHeight="80.0" prefWidth="351.0" style="-fx-background-color: #efefef; -fx-background-radius: 5;" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="iconImageView" fitHeight="33.0" fitWidth="35.0" layoutX="14.0" layoutY="23.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/icon_tasks.png" />
         </image>
      </ImageView>
      <Label fx:id="taskLabel" layoutX="58.0" layoutY="2.0" prefHeight="24.0" prefWidth="203.0" text="Task" textFill="#524bdd">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="descriptionLabel" layoutX="58.0" layoutY="27.0" prefHeight="24.0" prefWidth="274.0" text="Description" textFill="#524bdd" wrapText="true" />
      <Label fx:id="dateLabel" alignment="CENTER_RIGHT" layoutX="7.0" layoutY="53.0" prefHeight="17.0" prefWidth="339.0" style="-fx-background-color: #efefef;" text="Date Created" textFill="#524bdd" />
      <ImageView fx:id="deleteButton" fitHeight="24.0" fitWidth="33.0" layoutX="278.0" layoutY="2.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/icon_delete.png" />
         </image>
      </ImageView>
      <ImageView fx:id="listUpdateButton" fitHeight="24.0" fitWidth="24.0" layoutX="312.0" layoutY="2.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/icon_update.png" />
         </image>
      </ImageView>
   </children>
   <effect>
      <DropShadow />
   </effect>
</AnchorPane>
